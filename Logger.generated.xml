                    RETURN;
                END_IF
                
                highest := THIS^.values[1];
                FOR i := 2 TO THIS^.count DO
                    IF THIS^.values[i] &gt; highest THEN
                        highest := THIS^.values[i];
                    END_IF
                END_FOR
                
                GetHighestValue := highest;</xhtml>
                  </ST>
                </body>
                <addData />
