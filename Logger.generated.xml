          <data name="http://www.3s-software.com/plcopenxml/property" handleUnknown="implementation">
            <Property name="GetLowestValue" ObjectId="f040d45b-285c-4dca-a8a6-d865537ec464">
              <interface>
                <returnType>
                  <REAL />
                </returnType>
              </interface>
              <GetAccessor>
                <interface>
                  <localVars><variable name="i"><type><INT /></type></variable><variable name="lowest"><type><REAL /></type></variable></localVars>
                  <addData>
                    <data name="http://www.3s-software.com/plcopenxml/attributes" handleUnknown="implementation">
                      <Attributes>
                        <Attribute Name="monitoring" Value="call" />
                      </Attributes>
                    </data>
                  </addData>
                </interface>
                <body>
                  <ST>
                    <xhtml xmlns="http://www.w3.org/1999/xhtml">IF THIS^.count = 0 THEN
                    GetLowestValue := 0.0;
                    RETURN;
                END_IF
                
                lowest := THIS^.values[1];
                FOR i := 2 TO THIS^.count DO
                    IF THIS^.values[i] &lt; lowest THEN
                        lowest := THIS^.values[i];
                    END_IF
                END_FOR
                
                GetLowestValue := lowest;</xhtml>
                  </ST>
                </body>
                <addData />
              </GetAccessor>
              <addData />
            </Property>
          </data>
          <data name="http://www.3s-software.com/plcopenxml/property" handleUnknown="implementation">
            <Property name="GetHighestValue" ObjectId="0ed868e0-0924-4d6d-aeaf-24829607195f">
